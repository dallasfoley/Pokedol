{
  "routes": [
    {
      "src": "^/app/(.*)",
      "dest": "/app/$1"
    },
    {
      "src": "^/src/(.*)",
      "dest": "/src/$1"
    },
    {
      "src": "/(.*)",
      "headers": {
        "cache-control": "s-maxage=0"
      },
      "dest": "/app/index.html",
      "continue": true
    }
  ]
}
